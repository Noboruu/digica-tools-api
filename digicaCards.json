   \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"When this card is trashed from your Deck, gain 1 memory.\"\r\n      }, {\r\n        \"effectType\" : \"INHERITED\",\r\n        \"effectText\" : \"[When Attacking] You may reveal 1 purple card from your hand and place it on top of your deck.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-078\",\r\n      \"name\" : \"Espimon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/f\/f1\/P-078.png\/revision\/latest?cb=20230717170818\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[On Play] Reveal the top card of your opponent's security stack. If that card is a Digimon card, \uFF1CDraw 1\uFF1E (Draw 1 card from your deck). Place the revealed card on top of your opponent's security stack face down.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-079\",\r\n      \"name\" : \"Agumon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/f\/f9\/P-079.png\/revision\/latest?cb=20220730122111\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[On Play] If you have a red Tamer in play, delete 1 of your opponent's Digimon with 3000 DP or less.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-080\",\r\n      \"name\" : \"Labramon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/d\/d2\/P-080.png\/revision\/latest?cb=20220730122056\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[On Play] If you have a purple Tamer in play, delete 1 of your opponent's level 3 Digimon.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-081\",\r\n      \"name\" : \"Falcomon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/3\/3a\/P-081.png\/revision\/latest?cb=20220730122041\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[On Play] If you have a yellow Tamer in play, 1 of your opponent's Digimon gets -2000 DP until the end of their turn.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-082\",\r\n      \"name\" : \"Kunemon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/8\/82\/P-082.png\/revision\/latest?cb=20240330185453\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[On Play] If you have a green Tamer in play, suspend 1 of your opponent's Digimon.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-083\",\r\n      \"name\" : \"Floramon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/a\/ab\/P-083.png\/revision\/latest?cb=20240330185452\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[On Play] If you have a green Tamer in play, 1 of your opponent's Digimon can't unsuspend until the end of their turn.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-084\",\r\n      \"name\" : \"Lopmon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/3\/3f\/P-084.png\/revision\/latest?cb=20240330185452\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[On Play] If you have a yellow Tamer in play, 1 of your opponent's Digimon gains \uFF1CSecurity Attack -1\uFF1E (This Digimon checks 1 fewer security card) until the end of their turn.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-085\",\r\n      \"name\" : \"Dracmon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/6\/60\/P-085.png\/revision\/latest?cb=20240330185453\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[On Play] If you have a purple Tamer in play, if its your turn, you may digivolve this Digimon into a Digimon card with the [Undead]\/[Dark Animal] trait from your trash by paying its digivolution cost.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-086\",\r\n      \"name\" : \"Syakomon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/a\/a5\/P-086.png\/revision\/latest?cb=20240330185453\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[On Play] If you have a blue Tamer in play, 1 of your Digimon can't be attacked until the end of your opponent's turn.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-087\",\r\n      \"name\" : \"Ritsu Kodo\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/0\/07\/P-087.png\/revision\/latest?cb=20240308053805\",\r\n      \"cardType\" : \"TAMER\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[Your Turn] When you would play [Pulsemon], by suspending this tamer, if you have 3 or more security cards, \uFF1CDraw 1\uFF1E (Draw 1 card from your deck). If you have 3 or less security cards, gain 1 memory.\"\r\n      }, {\r\n        \"effectType\" : \"SECURITY\",\r\n        \"effectText\" : \"[Security] Play this card without paying the cost.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-088\",\r\n      \"name\" : \"Siriusmon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/e\/e1\/P-088.png\/revision\/latest?cb=20230901095557\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[When Digivolving] By placing 1 card with [Gammamon] in its name from your hand as this Digimon's bottom digivolution card, this Digimon gets +2000 DP for the turn. [When Attacking] Delete 1 of your opponent's Digimon with 6000 DP or less. If this Digimon has 12000 DP or more, Delete 2 such Digimon instead.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-089\",\r\n      \"name\" : \"Amphimon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/4\/42\/P-089.png\/revision\/latest?cb=20230901095557\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[When Digivolving] You may trash up to 3 blue cards from your hand. For each card trashed by this effect, you may trash 1 card under 1 of your opponent's Digimon or Tamers. Then, 1 of your opponent's Digimon or Tamers without cards under it can't suspend until the end of your opponent's turn. [Opponent's Turn] [Once Per Turn] When an opponent's Digimon attacks, end that attack by returning 3 cards with [Jellymon] in their text from your trash to the bottom of your deck.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-090\",\r\n      \"name\" : \"Diarbbitmon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/a\/a8\/P-090.png\/revision\/latest?cb=20230901095557\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[When Digivolving] Suspend 2 of your opponent's Digimon. [All Turns] [Once Per Turn] When one of your Digimon deletes an opponent's Digimon in battle and survives, if [Angoramon] is in this Digimon's digivolution cards, unsuspend 1 of your Digimon.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-091\",\r\n      \"name\" : \"Saberdramon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/a\/ae\/P-091.png\/revision\/latest?cb=20240121111552\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"\uFF1CRaid\uFF1E (When this Digimon attacks, you may switch the target of attack to 1 of your opponent's unsuspended Digimon with the highest DP). \uFF1CRetaliation\uFF1E (When this Digimon is deleted after losing a battle, delete the Digimon it was battling).\"\r\n      }, {\r\n        \"effectType\" : \"INHERITED\",\r\n        \"effectText\" : \"[On Deletion] Return 1 red or purple Digimon card from your trash to the hand.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-092\",\r\n      \"name\" : \"Dracomon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/6\/61\/P-092.png\/revision\/latest?cb=20240121111554\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[Your Turn] When you play a [Groundramon], this Digimon may digivolve into a [Wingdramon] from your hand for a cost of 3, ignoring its digivolution requirements.\"\r\n      }, {\r\n        \"effectType\" : \"INHERITED\",\r\n        \"effectText\" : \"[Your Turn] When you play a [Groundramon], this Digimon may digivolve into a [Wingdramon] from your hand without paying its cost.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-093\",\r\n      \"name\" : \"Bastemon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/9\/9e\/P-093.png\/revision\/latest?cb=20240121111555\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[All Turns] When this Digimon becomes suspended, suspend 1 of your opponent's Digimon.\"\r\n      }, {\r\n        \"effectType\" : \"INHERITED\",\r\n        \"effectText\" : \"[Your Turn] [Once Per Turn] When this Digimon would digivolve, reduce the digivolution cost by 1.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-094\",\r\n      \"name\" : \"Destromon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/8\/81\/P-094.png\/revision\/latest?cb=20240121111555\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[On Play] [When Digivolving] Delete your opponent's Digimon and Tamers with a total play cost of 3. For every [Vemmon] in this Digimon's digivolution cards, increase the maximum play cost you can choose by this effect by 1.\"\r\n      }, {\r\n        \"effectType\" : \"INHERITED\",\r\n        \"effectText\" : \"[Opponent's Turn] [Once Per Turn] When an opponent's Digimon attacks, by placing 2 [Vemmon] from 1 of your [Galacticmon]'s digivolution cards at the bottom of their owners' decks, switch the target of attack to this Digimon.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-095\",\r\n      \"name\" : \"Pause Plug-In P\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/8\/8a\/P-095.png\/revision\/latest?cb=20240121111555\",\r\n      \"cardType\" : \"OPTION\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"While you have a Tamer in play, you may use this card without meeting its color requirements. [Main] Until the end of your opponent's turn, 1 of your opponent's Digimon gets -6000 DP and that Digimon's [When Digivolving] effects don't activate.\"\r\n      }, {\r\n        \"effectType\" : \"SECURITY\",\r\n        \"effectText\" : \"[Security] 1 of your opponent's Digimon gets -6000 DP for the turn. Then, add this card to the hand.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-096\",\r\n      \"name\" : \"Prism Garrett\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/0\/0a\/P-096.png\/revision\/latest?cb=20240121111555\",\r\n      \"cardType\" : \"OPTION\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"While you have a Tamer with the [Hunter] trait in play, you may use this card without meeting its color requirements. [Main] By placing up to 2 Digimon cards with \uFF1CSave\uFF1E in their text from under your Tamers or your trash under 1 of your Digimon with \uFF1CSave\uFF1E in its text as its bottom digivolution cards, that Digimon gains +1000 DP for each card placed by this effect.\"\r\n      }, {\r\n        \"effectType\" : \"SECURITY\",\r\n        \"effectText\" : \"[Security] Add this card to the hand.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-097\",\r\n      \"name\" : \"Zubamon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/a\/a3\/P-097.png\/revision\/latest?cb=20240121111555\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[On Play] By placing this card under 1 of your other Digimon in play as its bottom digivolution card, reveal the top 3 cards of your deck. Place those cards at either the top or bottom of your deck in any order. Then, if you have a Digimon with the [Legend-Arms] trait in play, gain 2 memory.\"\r\n      }, {\r\n        \"effectType\" : \"INHERITED\",\r\n        \"effectText\" : \"[Your Turn] While you have a Digimon with the [Legend-Arms] trait or a black Digimon in play, this Digimon gains \uFF1CRaid\uFF1E (When this Digimon attacks, you may switch the target of attack to 1 of your opponent's unsuspended Digimon with the highest DP).\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-098\",\r\n      \"name\" : \"Seadramon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/1\/16\/P-098.png\/revision\/latest?cb=20240121111555\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[On Play] [When Digivolving] 1 of your blue Digimon cannot be deleted in battle until the end of your opponent's turn.\"\r\n      }, {\r\n        \"effectType\" : \"INHERITED\",\r\n        \"effectText\" : \"[Your Turn] [Once Per Turn] When you play another Digimon by an effect, 1 of your blue Digimon gains \uFF1CRush\uFF1E (This Digimon can attack the turn it comes into play) for the turn.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-099\",\r\n      \"name\" : \"Etemon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/2\/2f\/P-099.png\/revision\/latest?cb=20240121111556\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[On Play] [When Digivolving] \uFF1CDe-Digivolve 1\uFF1E 1 of your opponent's Digimon (Trash the top card. You can't trash past level 3 cards).\"\r\n      }, {\r\n        \"effectType\" : \"INHERITED\",\r\n        \"effectText\" : \"[On Deletion] You may play 1 yellow or black Digimon card with a play cost of 3 or less from your hand without paying the cost.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-100\",\r\n      \"name\" : \"Kuwagamon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/2\/20\/P-100.png\/revision\/latest?cb=20240121111556\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[On Play] [When Digivolving] 1 of your opponent's Digimon or Tamers cannot unsuspend until the end of your opponent's turn.\"\r\n      }, {\r\n        \"effectType\" : \"INHERITED\",\r\n        \"effectText\" : \"[Your Turn] This Digimon gets +2000 DP.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-101\",\r\n      \"name\" : \"Raremon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/b\/be\/P-101.png\/revision\/latest?cb=20240121111556\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"This card\/Digimon is also treated as having the [Cyborg] trait. [On Play] [When Digivolving] By trashing 1 card from your hand, \uFF1CDraw 2\uFF1E (Draw 2 cards from your deck).\"\r\n      }, {\r\n        \"effectType\" : \"INHERITED\",\r\n        \"effectText\" : \"[When Attacking] By trashing 1 card from your hand, delete 1 of your opponent's level 3 Digimon.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-102\",\r\n      \"name\" : \"SkullGreymon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/4\/46\/P-102.png\/revision\/latest?cb=20240121111556\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[On Play] [When Digivolving] By deleting 1 of your Digimon, delete 2 of your opponent's Digimon with 5000 DP or less. [On Deletion] You may play 1 purple or red level 3 Digimon card from your trash without paying the cost.\"\r\n      }, {\r\n        \"effectType\" : \"INHERITED\",\r\n        \"effectText\" : \"[On Deletion] You may play 1 red or purple level 3 Digimon card from your trash without paying the cost.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"P-103\",\r\n      \"name\" : \"Offense Training\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/0\/0f\/P-103.png\/revision\/latest?cb=20230421114301\",\r\n      \"cardType\" : \"OPTION\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"[Main] Reveal the top 2 cards of your deck. Add 1 red card among them to your hand. Place the rest at the bottom of your deck in any order. Then, place this card into your battle area. [Main] \uFF1CDelay\uFF1E (After this card is placed, by trashing it the next turn or later, activate the effect below.) \u30FB1 of your Digimon may digivolve into a red Digimon car