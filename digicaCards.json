     \"name\" : \"Starmon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/5\/51\/BT9-035.png\/revision\/latest?cb=20220625065055\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ ]\r\n    }, {\r\n      \"code\" : \"BT9-036\",\r\n      \"name\" : \"Gatomon (X Antibody)\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/7\/77\/BT9-036.png\/revision\/latest?cb=20220625065055\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"INHERITED\",\r\n        \"effectText\" : \"[When Attacking] If you have 3 or more security cards, 1 of your opponent's Digimon gets -2000 DP for the turn.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"BT9-037\",\r\n      \"name\" : \"Nefertimon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/c\/c9\/BT9-037.png\/revision\/latest?cb=20220625065055\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"\uFF1CArmor Purge\uFF1E (When this Digimon would be deleted, you may trash the top card of this Digimon to prevent that deletion). [When Attacking] 1 of your opponent's Digimon gets -2000 DP for the turn.\"\r\n      } ]\r\n    }, {\r\n      \"code\" : \"BT9-038\",\r\n      \"name\" : \"Pegasusmon\",\r\n      \"artUrl\" : \"https:\/\/static.wikia.nocookie.net\/digimoncardgame\/images\/2\/27\/BT9-038.png\/revision\/latest?cb=20220625065102\",\r\n      \"cardType\" : \"DIGIMON\",\r\n      \"cardEffects\" : [ {\r\n        \"effectType\" : \"CARD\",\r\n        \"effectText\" : \"\uFF1CArmor Purge\uFF1E (When this Digimon would be deleted, you may trash the top card of this Digimon to prevent that deletion). [When Digivolving] 1 of your opponent's Digimon gains \uFF1CSecurity Attack -1\uFF1E (This Digimon checks 1 fewer security card) until the end of your opponent's turn.\"\r\n      } ]\